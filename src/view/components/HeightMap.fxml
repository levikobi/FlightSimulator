<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import view.library.HeightMap?>
<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.components.HeightMapController">
    <HeightMap
            fx:id="heightMap"
            height="350.0"
            width="400.0"
            onMouseClicked="#handleClickMapDisplayer"
            characterImageName="./resources/airplane.png"
            destinationImageName="./resources/destination.png"
    >
        <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </VBox.margin>
    </HeightMap>
</VBox>
